     1                                  ; Ne propunem ca programul de mai jos sa citeasca de la tastatura un numar si sa afiseze pe ecran valoarea numarului citit impreuna cu un mesaj.
     2                                  bits 32
     3                                  global start        
     4                                  
     5                                  ; declararea functiilor externe folosite de program
     6                                  extern exit, printf, scanf  ; adaugam printf si scanf ca functii externe           
     7                                  import exit msvcrt.dll     
     8                                  import printf msvcrt.dll     ; indicam asamblorului ca functia printf se gaseste in libraria msvcrt.dll
     9                                  import scanf msvcrt.dll      ; similar pentru scanf
    10                                                            
    11                                  segment  data use32 class=data
    12 00000000 00000000                	n db 0,0,0,0;exitindem la DWORD ca sa nu dea overflow       ; in aceasta variabila se va stoca valoarea citita de la tastatura
    13 00000004 00000000                	m db 0,0,0,0
    14 00000008 416E61206172652025-         message  db "Ana are %d mere si %d pere", 0  
    14 00000011 64206D657265207369-
    14 0000001A 202564207065726500 
    15 00000023 256420256400            	format  db "%d %d", 0  ; %d <=> un numar decimal (baza 10)
    16                                      
    17                                  segment  code use32 class=code
    18                                      start:
    19                                                                                 
    20                                          ; vom apela scanf(format, n) => se va citi un numar in variabila n
    21                                          ; punem parametrii pe stiva de la dreapta la stanga
    22 00000000 68[04000000]                    push dword m
    23 00000005 68[00000000]                    push  dword n       ; ! adresa lui n, nu valoarea
    24 0000000A 68[23000000]                    push  dword format
    25 0000000F FF15[00000000]                  call  [scanf]       ; apelam functia scanf pentru citire
    26 00000015 83C40C                          add  esp, 4 * 3     ; eliberam parametrii de pe stiva; 4 = dimensiunea unui dword; 2 = nr de parametri
    27                                          
    28                                          ;convertim n la dword pentru a pune valoarea pe stiva 
    29 00000018 B800000000                      mov  eax,0
    30 0000001D BB00000000                      mov ebx,0
    31 00000022 A0[00000000]                    mov  al,[n]
    32 00000027 8A1D[04000000]                  mov bl, [m]
    33                                          
    34                                          ;afisam mesajul si valoarea lui n
    35 0000002D 53                              push ebx
    36 0000002E 50                              push  eax
    37 0000002F 68[08000000]                    push  dword message 
    38 00000034 FF15[00000000]                  call  [printf]
    39 0000003A 83C408                          add  esp,4*2 
    40                                          
    41                                          ; exit(0)
    42 0000003D 6A00                            push  dword 0     ; punem pe stiva parametrul pentru exit
    43 0000003F FF15[00000000]                  call  [exit]       ; apelam exit pentru a incheia programul
